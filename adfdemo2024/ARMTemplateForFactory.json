		"ls_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2sa2024.dfs.core.windows.net/"
		},
		"Web1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.careerpower.in/states-and-capitals-of-india.html"
		}
